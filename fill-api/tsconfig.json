{
    "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src"
    },
    "include": ["src/**/*", "generated/client/**/*"],
    "exclude": ["node_modules", "dist", "generated"]
}